import matplotlib.pyplot as plt

months = ['Jan', 'Feb', 'Mar', 'Apr']
sales = [1000, 1500, 1700, 2000]

plt.plot(months, sales)
plt.title("Monthly Sales (Line Plot)")
plt.xlabel("Month")
plt.ylabel("Sales")
plt.show()

plt.bar(months, sales, color='orange')
plt.title("Monthly Sales (Bar Plot)")
plt.xlabel("Month")
plt.ylabel("Sales")
plt.show()


